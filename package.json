{
  "name": "ng-picathu",
  "version": "1.3.1",
  "private": false,
  "description": "NGX-Picathu-utils is a library for angular that contains a lot of helper functions that can be used in any angular project",
  "homepage": "https://github.com/PoulaHelmy/ng-picathu.git#readme",
  "bugs": {
    "url": "https://github.com/PoulaHelmy/ng-picathu.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PoulaHelmy/ng-picathu.git"
  },
  "license": "MIT",
  "author": "Poula Helmy <poula.helmy.mousa@gmail.com>",
  "contributors": [
    "Poula Helmy <poula.helmy.mousa@gmail.com>"
  ],
  "scripts": {
    "build": "ng build",
    "commit": "git-cz",
    "format": "prettier \"**/*\" --ignore-unknown",
    "format:write": "pnpm format --write",
    "lint:es": "eslint . .*js --max-warnings 0",
    "lint:fix": "ng lint --fix",
    "lint:knip": "knip",
    "lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
    "lint:ng": "ng lint",
    "lint:package": "npmPkgJsonLint .",
    "lint:package-json": "npmPkgJsonLint .",
    "lint:packages": "pnpm dedupe --check",
    "lint:prune": "ts-prune",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "lint:styles": "stylelint \"**/*.{css,scss}\"",
    "lint:styles:fix": "stylelint --fix \"**/*.{css,scss}\"",
    "ng": "ng",
    "prepare": "husky install",
    "release": "release-it",
    "should-semantic-release": "should-semantic-release --verbose",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@release-it/conventional-changelog": "^7.0.1",
    "@types/eslint": "^8.44.2",
    "@types/git-url-parse": "^9.0.1",
    "@types/jasmine": "~4.3.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.6.0",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "commitizen": "^4.3.0",
    "cspell": "^7.3.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "knip": "^2.22.0",
    "ng-packagr": "^15.2.2",
    "npm-package-json-lint": "^7.0.0",
    "npm-package-json-lint-config-default": "^6.0.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-curly": "^0.1.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "release-it": "^16.1.5",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "ts-prune": "^0.10.3",
    "typescript": "~4.9.4",
    "validate-branch-name": "^1.3.0",
    "yaml-eslint-parser": "^1.2.2"
  }
}
